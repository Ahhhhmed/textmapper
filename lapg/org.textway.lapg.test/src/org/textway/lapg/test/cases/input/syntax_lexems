lang = "java"
prefix = "Lexems"

identifier: /@?[a-zA-Z_][A-Za-z_0-9]*/   -2    (class)
identifierX: /@[a-z]+/       			 -1	 (class)
_skip:      /[\t\r\n ]+/    							{ return false; }

Lclass:		 /class/
Lextends:	 /extends/

LInt:		 /Int/               (soft)

Lstring:	 /string/

Lset:		 /set/
Lchoice:	 /choice/

# err: two matches
Lbool:		 /@bool/				(soft)

# err: no match!
L0choice:	 /0choice/			(soft)

# err: not a constant!
int:	 /[0-9]+/			(soft)





input ::=
	identifier ;
