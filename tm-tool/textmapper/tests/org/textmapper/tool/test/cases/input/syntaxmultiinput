language syntaxmultiinput(java);

lang = "java"
prefix = "AA"

:: lexer

identifier(String): /[a-zA-Z_][a-zA-Z_0-9]*/ -1 { $lexem = current(); break; }
_skip:          /[\n\t\r ]+/                   	(space)

AAA: /aaa/

:: parser

%input A1, A2;

A1 ::=
	AAA
   	|
	A2 identifier ;

A2 ::=
	A1
	| A2 AAA ;

%%