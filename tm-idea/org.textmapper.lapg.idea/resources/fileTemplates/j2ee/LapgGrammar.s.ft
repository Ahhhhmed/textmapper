# lapg syntax file

lang = "java"
prefix = "${NAME}"
#if (${PACKAGE_NAME} != "")package = "${PACKAGE_NAME}"
#end

identifier(String): /[a-zA-Z_][a-zA-Z_0-9]*/   { $lexem = current(); break; }
icon(Integer):  /-?[0-9]+/                     { $lexem = Integer.parseInt(current()); break; }
_skip:          /[\n\t\r ]+/                   { return false; }

# grammar

input ::=
	identifier ;

%%